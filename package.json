{
  "name": "remark-fenced-divs",
  "version": "1.1.4",
  "description": "remark plugin to enable pandoc fenced_divs",
  "license": "MIT",
  "keywords": [
    "unified",
    "remark",
    "remark-plugin",
    "plugin",
    "mdast",
    "markdown",
    "compile",
    "html",
    "stringify"
  ],
  "repository": "benabel/remark-fenced-divs",
  "bugs": "https://github.com/benabel/remark-fenced-divs/issues",
  "author": "Benjamin Abel <dev.abel@free.fr> (https://lyceum.fr)",
  "contributors": [
    "Benjamin Abel <dev.abel@free.fr> (https://lyceum.fr)"
  ],
  "types": "types/index.d.ts",
  "files": [
    "types/index.d.ts",
    "index.js"
  ],
  "dependencies": {
    "micromark": "~2.10.0",
    "parse-entities": "^2.0.0",
    "repeat-string": "^1.0.0",
    "stringify-entities": "^3.1.0",
    "unist-util-visit-parents": "^3.0.0"
  },
  "devDependencies": {
    "dtslint": "^4.0.0",
    "nyc": "^15.0.0",
    "onchange": "^7.1.0",
    "prettier": "^2.0.0",
    "rehype": "^11.0.0",
    "rehype-format": "^3.1.0",
    "rehype-stringify": "^8.0.0",
    "remark": "^13.0.0",
    "remark-cli": "^9.0.0",
    "remark-parse": "^9.0.0",
    "remark-preset-wooorm": "^8.0.0",
    "remark-rehype": "^8.0.0",
    "tape": "^5.0.0",
    "to-vfile": "^6.0.0",
    "unified": "^9.0.0",
    "unist-util-remove-position": "^3.0.0",
    "xo": "^0.34.0"
  },
  "scripts": {
    "format": "remark . -qfo --ignore-pattern test/ && prettier . -w --loglevel warn && xo --fix",
    "start": "onchange **/**.js -- npm run test-api",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test/index.js",
    "test-types": "dtslint types",
    "test": "npm run format && npm run test-coverage && npm run test-types",
    "test:e2e": "node test/e2e/test-e2e.js"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false,
    "rules": {
      "unicorn/prefer-includes": "off",
      "no-eq-null": "off",
      "eqeqeq": [
        "error",
        "always",
        {
          "null": "ignore"
        }
      ]
    }
  }
}
