{
  "name": "remark-fenced-divs",
  "version": "1.1.3",
  "description": "remark plugin to enable pandoc fenced_divs",
  "license": "MIT",
  "keywords": [
    "unified",
    "remark",
    "remark-plugin",
    "plugin",
    "mdast",
    "markdown",
    "compile",
    "html",
    "stringify"
  ],
  "repository": "benabel/remark-fenced-divs",
  "bugs": "https://github.com/benabel/remark-fenced-divs/issues",
  "author": "Benjamin Abel <dev.abel@free.fr> (https://lyceum.fr)",
  "contributors": [
    "Benjamin Abel <dev.abel@free.fr> (https://lyceum.fr)"
  ],
  "files": [
    "index.js"
  ],
  "devDependencies": {
    "eslint-plugin-prettier": "^3.1.4",
    "is-hidden": "^1.0.0",
    "negate": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.0.2",
    "prettier": "^2.1.2",
    "rehype": "^11.0.0",
    "rehype-format": "^3.1.0",
    "rehype-stringify": "^8.0.0",
    "remark-cli": "^8.0.1",
    "remark-rehype": "^7.0.0",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.1",
    "to-vfile": "^6.1.0",
    "vfile": "^4.2.0",
    "vfile-reporter": "^6.0.1",
    "xo": "^0.33.1"
  },
  "peerDependencies": {
    "remark": "<13"
  },
  "scripts": {
    "format": "remark . -qfo && prettier --write \"**/*.js\" && xo --fix",
    "start": "onchange -i 'index.js' 'test/test*.js' -- yarn test",
    "test": "tape ./test/*.js | tap-spec",
    "test:e2e": "node test/e2e/test-e2e.js"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false,
    "rules": {
      "unicorn/prefer-includes": "off",
      "no-eq-null": "off",
      "eqeqeq": [
        "error",
        "always",
        {
          "null": "ignore"
        }
      ]
    }
  }
}
