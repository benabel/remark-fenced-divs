{
  "name": "remark-fenced-divs",
  "version": "0.0.1",
  "description": "remark plugin to enable fenced_divs",
  "license": "MIT",
  "keywords": [
    "unified",
    "remark",
    "remark-plugin",
    "plugin",
    "mdast",
    "markdown",
    "compile",
    "html",
    "stringify"
  ],
  "repository": "benabel/remark-fenced-divs",
  "bugs": "https://github.com/benabel/remark-fenced-divs/issues",
  "author": "Benjamin Abel <dev.abel@free.fr> (https://lyceum.fr)",
  "contributors": [
    "Benjamin Abel <dev.abel@free.fr> (https://lyceum.fr)"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "@mapbox/hast-util-table-cell-style": "^0.1.3",
    "hast-to-hyperscript": "^8.0.0",
    "hast-util-sanitize": "^2.0.0",
    "mdast-util-to-hast": "^8.0.0"
  },
  "devDependencies": {
    "eslint-plugin-prettier": "^3.1.3",
    "is-hidden": "^1.0.0",
    "negate": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0",
    "prettier": "^2.0.0",
    "rehype-stringify": "^7.0.0",
    "remark": "^12.0.0",
    "remark-rehype": "^6.0.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.0.0",
    "vfile": "^4.0.0",
    "xo": "^0.28.0"
  },
  "scripts": {
    "sub-install": "cd test/fenced_divs/v16 && npm install && cd ../..",
    "format": "remark . -qfo && prettier --write \"**/*.js\" && xo --fix",
    "start": "onchange -i 'index.js' 'test/test*.js' -- yarn test",
    "test": "tape ./test/*.js | tap-spec",
    "test:e2e": "npm-run-all test:e2e:* && prettier --write test/e2e/*.html",
    "test:e2e:pandoc": "pandoc -f markdown-auto_identifiers test/e2e/test.md -o test/e2e/test-pandoc.html",
    "test:e2e:remark": "cd test/e2e/ && node test-e2e"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false,
    "rules": {
      "unicorn/prefer-includes": "off",
      "no-eq-null": "off",
      "eqeqeq": [
        "error",
        "always",
        {
          "null": "ignore"
        }
      ]
    }
  }
}